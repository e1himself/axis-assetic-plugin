{
  "name": "axis/axis-assetic-plugin",
  "description": "symfony 1.x plugin that integrates Assetic into symfony",
  "type": "symfony1-plugin",
  "keywords": ["symfony", "assetic", "asset manager", "minifier"],
  "license": "MIT",
  "homepage": "https://github.com/e1himself/axis-assetic-plugin",
  "authors": [
    {
      "name": "Ivan Voskoboynyk",
      "email": "ioann.voskoboynyk@gmail.com",
      "homepage": "https://github.com/e1himself"
    }
  ],
  "version": "1.0.0",

  "minimum-stability": "dev",

  "autoload": {
    "psr-0": {
      "Axis\\S1\\Assetic": "lib/"
    }
  },

  "require": {
    "php": ">=5.3.0",
    "composer/installers": "*",
    "kriswallsmith/assetic": "dev-master",
    "axis/axis-service-container-plugin": "dev-master"
  },

  "suggest": {
    "natxet/CssMin": "Needed to use CssMin asset filter",
    "leafo/lessphp": "Needed to use LessPHP compiler",
    "nick4fake/jsmin": "Needed for JsMin filter",
    "ptachoire/cssembed": "Needed for PhpCssEmbed filter",
    "leafo/scssphp": "Needed for ScssPhp filter"
  }
}